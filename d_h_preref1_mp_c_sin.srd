         h_fmp_sin.co_calidad,   
         m_disenos.co_diseno,   
         m_disenos.de_diseno  
    from h_preref,   
         dt_preref,   
         h_fmp_sin,   
         m_disenos  
   where ( dt_preref.co_fabrica = h_preref.co_fabrica ) and  
         ( dt_preref.co_linea = h_preref.co_linea ) and  
         ( dt_preref.co_referencia = h_preref.co_referencia ) and  
