table(column=(type=long updatewhereclause=yes key=yes name=nu_orden dbname="dt_mu_ord.nu_orden" )
 column=(type=long updatewhereclause=yes name=m_muestras_co_muestra dbname="m_muestras.co_muestra" )
 column=(type=long updatewhereclause=yes name=dt_muestras_co_talla dbname="dt_muestras.co_talla" )
 column=(type=long updatewhereclause=yes name=m_muestras_co_cliente dbname="m_muestras.co_cliente" )
 column=(type=char(50) updatewhereclause=yes name=nom_cliente dbname="nom_cliente" )
 column=(type=long updatewhereclause=yes name=dt_muestras_co_recurso dbname="dt_muestras.co_recurso" )
 column=(type=char(30) updatewhereclause=yes name=de_recurso dbname="de_recurso" )
 column=(type=long updatewhereclause=yes name=dt_mu_ord_prioridad dbname="dt_mu_ord.prioridad" )
 column=(type=long update=yes updatewhereclause=yes name=condicion dbname="dt_mu_ord.condicion" )
