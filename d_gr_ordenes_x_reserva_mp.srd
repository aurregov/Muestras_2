 column=(type=datetime update=yes updatewhereclause=yes name=fe_actualizacion dbname="dt_mu_ord.fe_actualizacion" )
 column=(type=char(8) update=yes updatewhereclause=yes name=usuario dbname="dt_mu_ord.usuario" )
 column=(type=char(20) update=yes updatewhereclause=yes name=instancia dbname="dt_mu_ord.instancia" )
 column=(type=char(20) updatewhereclause=yes name=tarea dbname="tarea" )
 column=(type=date updatewhereclause=yes name=fe_entrega_tejido dbname="fe_entrega_tejido" )
 column=(type=date updatewhereclause=yes name=fe_entrega_cliente dbname="fe_entrega_cliente" )
 column=(type=long updatewhereclause=yes name=estado_fmp dbname="estado_fmp" values="	1/	0" )
 column=(type=long updatewhereclause=yes name=agrupa_orden dbname="agrupa_orden" )
 column=(type=char(6) updatewhereclause=yes name=estado_mp dbname="estado_mp" )
 column=(type=long updatewhereclause=yes name=sw_reserva dbname="sw_reserva" values="	1/	0" )
 column=(type=long update=yes updatewhereclause=yes name=co_reserva_sap dbname="dt_mu_ord.co_reserva_sap" )
 column=(type=long updatewhereclause=yes name=sw_pedir_mp dbname="sw_pedir_mp" values="	1/	0" )
 column=(type=long updatewhereclause=yes name=estado_reserva dbname="estado_reserva" )
 column=(type=decimal(0) updatewhereclause=yes name=ca_programada_orden dbname="ca_programada_orden" )
 column=(type=long updatewhereclause=yes name=sw_fmp_x_reserva dbname="sw_fmp_x_reserva" )
 column=(type=long updatewhereclause=yes name=co_actividad dbname="co_actividad" )
 column=(type=long updatewhereclause=yes name=co_actividad_operacion dbname="co_actividad_operacion" )
 retrieve="Select 
	dt_mu_ord.nu_orden, 
	m_muestras.co_muestra, 
	dt_muestras.co_talla,
	m_muestras.co_cliente, 
	sp_nom_cliente(m_muestras.tipo_muestra, m_muestras.co_cliente, m_muestras.co_sucursal ) as nom_cliente,
	dt_muestras.co_recurso, 
	sp_nom_m_recursos_tj(dt_mu_ord.co_recurso) as de_recurso,
	dt_mu_ord.prioridad,
	dt_mu_ord.condicion,	
	dt_mu_ord.indicador_mp,	
	dt_mu_ord.fe_actualizacion,   
	dt_mu_ord.usuario,   
	dt_mu_ord.instancia,   
	(Select Trim(sp_nom_m_est_mue(tmp_gantt.co_estado)) 
		From (
				Select first 1 *
				From mu_gantt
				Where mu_gantt.nu_orden = dt_mu_ord.nu_orden And 
					( fe_ini IS NOT NULL OR consec <= 2)
				Order by fe_ini Desc, consec
			) tmp_gantt
	) Tarea,
	Cast( '' as Date ) fe_entrega_tejido,
	Cast( sp_mu_fpfin_max(dt_mu_ord.nu_orden) as Date ) fe_entrega_cliente,
	Case When /* Se verifica que tenga ficha de materia prima en todas las pintas de la orden */
		(Select count(*)
			From dt_mu_ord_col
			Where nu_orden = dt_mu_ord.nu_orden And 
				dt_mu_ord_col.pares_tejer > 0 And
				(	Select  Count(*)
					From dt_fmp_muestras
					where co_fabrica = m_muestras.co_fabrica And
						co_linea = m_muestras.co_linea And
						co_muestra = m_muestras.co_muestra And
						co_talla = dt_muestras.co_talla And
						co_color = dt_mu_ord_col.co_color
				) = 0
			) = 0 Then
			1
		When /* Se verifica que tenga ficha de materia prima o fmp base en todas las pintas de la orden */
		(Select count(*)
			From dt_mu_ord_col
			Where nu_orden = dt_mu_ord.nu_orden And 
				dt_mu_ord_col.pares_tejer > 0 And
				(	Select  Count(*)
					From dt_fmp_muestras
					where co_fabrica = m_muestras.co_fabrica And
						co_linea = m_muestras.co_linea And
						co_muestra = m_muestras.co_muestra And
						co_talla = dt_muestras.co_talla And
						co_color = dt_mu_ord_col.co_color
				) = 0 And
				(	Select  Count(*)
					From dt_fmp_mue1
					where co_fabrica = m_muestras.co_fabrica And
						co_linea = m_muestras.co_linea And
						co_muestra = m_muestras.co_muestra And
						co_talla = dt_muestras.co_talla And
						co_color = dt_mu_ord_col.co_color
				) = 0
			) = 0 Then
			1
		Else 0
	End estado_fmp,
	( dt_mu_ord.co_reserva_sap) agrupa_orden,
	'      ' estado_mp,
	0 sw_reserva,
	dt_mu_ord.co_reserva_sap,
	0 sw_pedir_mp,
	0 estado_reserva,
	(Select SUM( pares_tejer )*2
		From dt_mu_ord_col
		Where nu_orden = dt_mu_ord.nu_orden
	) ca_programada_orden,
	( 1 ) sw_fmp_x_reserva,
	(Select id_actividad
		From (
				Select first 1 *
				From mu_gantt
				Where mu_gantt.nu_orden = dt_mu_ord.nu_orden And 
					( fe_ini IS NOT NULL OR consec <= 2)
				Order by fe_ini Desc, consec
			) tmp_gantt
	) co_actividad,
	(Select NVL( id_actividad, co_operacion)
		From (
				Select first 1 *
				From mu_gantt
				Where mu_gantt.nu_orden = dt_mu_ord.nu_orden And 
					( fe_ini IS NOT NULL OR consec <= 2)
				Order by fe_ini Desc, consec
			) tmp_gantt
	) co_actividad_operacion


From dt_mu_ord,   
   m_muestras,
	dt_muestras 

where dt_mu_ord.condicion in ( 2, 3 ) And
	dt_mu_ord.indicador_mp IN ( 0, 1 ) And
	m_muestras.co_fabrica = dt_mu_ord.co_fabrica And
	m_muestras.co_linea = dt_mu_ord.co_linea And
	m_muestras.co_muestra = dt_mu_ord.co_muestra And
	dt_muestras.co_fabrica = m_muestras.co_fabrica And
	dt_muestras.co_linea = m_muestras.co_linea And
	dt_muestras.co_muestra = m_muestras.co_muestra
;" update="dt_mu_ord" updatewhere=1 updatekeyinplace=yes  sort="IsNull(fe_entrega_tejido) A fe_entrega_cliente A fe_entrega_tejido A If( agrupa_orden > 0, 0, 1 ) A nu_orden A " )
